            // å‘å¸ƒCucumberæŠ¥å‘Š
            cucumber reportTitle: 'Cucumber Test Report',
                     jsonReportDirectory: 'reports',
                     fileIncludePattern: '**/*.json',
                     trendsLimit: 10
